{
  "name": "personal-site",
  "version": "0.1.0",
  "type": "commonjs",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "check-lint": "next lint",
    "check-types": "tsc --pretty --noEmit",
    "check-format": "prettier --check ./src",
    "format": "prettier --write ./src",
    "prepare": "husky install",
    "test-all": "yarn check-lint && yarn check-types yarn check-format",
    "contentful-typescript-codegen": "contentful-typescript-codegen --output src/@types/generated/cms.d.ts"
  },
  "dependencies": {
    "@contentful/rich-text-react-renderer": "^15.2.0",
    "classnames": "^2.3.1",
    "contentful": "^8.5.0",
    "gsap": "^3.7.1",
    "next": "11.1.0",
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "devDependencies": {
    "@svgr/webpack": "^5.5.0",
    "@types/react": "17.0.19",
    "@typescript-eslint/eslint-plugin": "^4.29.3",
    "@typescript-eslint/parser": "^4.29.3",
    "contentful-management": "^7.33.0",
    "contentful-typescript-codegen": "^3.2.2",
    "dotenv": "^10.0.0",
    "eslint": "^7.32.0",
    "eslint-config-next": "11.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-react": "^7.24.0",
    "husky": "^7.0.0",
    "prettier": "^2.3.2",
    "sass": "^1.38.2",
    "typescript": "4.3.5"
  }
}
